    "test:watch": "vitest"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.456.0",
    "next": "14.2.5",
    "prisma": "5.20.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "2.12.7",
    "tailwind-merge": "2.5.4",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.16.3",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.45",
    "tailwindcss": "3.4.13",
    "tsx": "4.19.1",
    "typescript": "5.5.4",
    "vitest": "2.1.1"
  }
